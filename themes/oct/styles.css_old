/*---JS Select---*/
.select-outline {
	background: #ccc url(../images/select-arrow.gif) no-repeat 100% 50%;
	height: 31px;
	display: inline-block;
	position: relative;
	vertical-align: middle;
  width: 356px;
  z-index: 200;
}
.select-active {
  z-index: 300;
}
.select-outline div.select-text {
	height: 31px;
	font-size: 13px;
	display: inline-block;
	padding: 0 42px 0 7px;
  width: 307px;
  cursor: pointer;
	vertical-align: top;
  text-transform: none;
}
.select-outline .sub-optgroup {
    font-size: 16px;
    font-weight: bold;
}
.select-outline div.select-text span {
	display: inline-block;
	color: #040404;
	height: 31px;
	line-height: 31px;
	white-space: nowrap;
	overflow: hidden;
  cursor: pointer;
}
.sub-select {
  background-color: #ccc;
  width: 356px;
	position: absolute;
	top: 31px;
	max-height: 191px;
	overflow: auto;
	display: none;
	z-index: 400;
}
.sub-option {
	display: block;
	cursor: pointer;
	color: #000;
	padding: 0 7px 0 15px;
	white-space: nowrap;
	font-size: 13px;
	line-height: 31px;
  text-transform: none;
  border-bottom: 1px solid #c47500;
}
.sub-optgroup {
  background-color: #DFDFDF;
  display: block;
  cursor: default;
  color: #000;
  padding: 0 5px;
  white-space: nowrap;
  font-size: 15px;
  font-weight: bold;
  line-height: 31px;
  text-transform: none;
  border-bottom: 1px solid #c47500;
}
.active-sub-option,
.sub-option:hover {
  background: #eb9823;
  color: #fff;
}
.sub-top, .sub-bot {
	display: none
}
/*---End JS Select---*/

