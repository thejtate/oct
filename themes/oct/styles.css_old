@import 'fonts.css';

.body {
	background: url(images/bg.png) repeat 0 0;
	text-align: center;
	font: normal 12px Verdana, Arial, FreeSans, sans-serif;
	color: #515894;
}
a {
	text-decoration: none;
}
.clr:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.center {
	text-align: center;
}
#page {
	width: 944px;
	margin: 0 auto;
	text-align: left;
}

/*---Header---*/
#header {
	width: 944px;
	height: 251px;
	position: relative;
}
#header-inner > img {
	position: absolute;
	top: 0;
	left: 0;
}
#logo {
	width: 364px;
	height: 251px;
	position: absolute;
	left: 40px;
	top: 0;
}
/*---end Header---*/

/*---Nav---*/
#nav {
	background: url(images/bg-menu.png) no-repeat 1px 0;
	width: 944px;
	height: 66px;
	position: relative;
	z-index: 50;
}
#nav ul {
	list-style: none;
}
#nav>ul {
	padding-left: 40px;
}
#nav>ul>li {
	float: left;
	height: 55px;
	padding-right: 10px;
	top: -9px;
	position: relative;
}
#nav>ul>li>span {
	display: inline-block;
	height: 44px;
	padding-left: 10px;
	padding-top: 11px;
}
#nav>ul>li a {
	display: inline-block;
	font: bold 18px/44px 'HumanaSansITC';
	color: #ffb299;
}
#nav>ul>li a b {
	display: inline-block;
	text-shadow: -2px 2px 0 #7f2c32;
	cursor: pointer;
}
#nav>ul>li:hover>span>a,
#nav>ul>li a:hover {
	color: #fbffcc;
}
#nav>ul>li:hover ul {
	display: block;
}
#nav>ul>li.item2 {
	height: 56px;
}
#nav>ul>li.item2>span {
	height: 45px;
}
#nav>ul>li.item2>span>a {
	padding-bottom: 1px;
}
#nav>ul>li.item3 {
	height: 57px;
}
#nav>ul>li.item3>span {
	height: 46px;
}
#nav>ul>li.item3>span>a {
	padding-bottom: 2px;
}
#nav>ul>li.item4 {
	height: 58px;
}
#nav>ul>li.item4>span {
	height: 47px;
}
#nav>ul>li.item4>span>a {
	padding-bottom: 3px;
}
#nav>ul>li.item5 {
	height: 59px;
}
#nav>ul>li.item5>span {
	height: 48px;
}
#nav>ul>li.item5>span>a {
	padding-bottom: 4px;
}
#nav>ul>li.item6 {
	height: 61px;
}
#nav>ul>li.item6>span {
	height: 50px;
}
#nav>ul>li.item6>span>a {
	padding-bottom: 6px;
}
#nav>ul>li.item7 {
	height: 62px;
}
#nav>ul>li.item7>span {
	height: 51px;
}
#nav>ul>li.item7>span>a {
	padding-bottom: 7px;
}
#nav>ul>li.item8 {
	height: 64px;
}
#nav>ul>li.item8>span {
	height: 53px;
}
#nav>ul>li.item8>span>a {
	padding-bottom: 9px;
}
#nav>ul>li.active {
	background: url(images/menu-right.png) no-repeat 100% 0;
	padding-right: 13px;
}
#nav>ul>li.active>span {
	background: url(images/menu-left.png) no-repeat 0 0;
	padding-left: 17px;
}
#nav>ul>li.active>span>a {
	background: url(images/menu-arrow.png) no-repeat 50% 100%;
	display: inline-block;
	height: 64px;
	margin-bottom: -20px;
	padding-left: 5px;
}
#nav>ul>li.active>span>a b,
#nav>ul>li.active>span>a:hover b {
	color: #fff;
	text-shadow: -2px 2px 0 #375a88;
}
#nav ul ul {
	background: none;
	background: rgba(0,0,0,0.25);
	padding: 0 0 5px 8px;
	position: absolute;
	top: 55px;
	left: 0;
	display: none;
	z-index: 60;
}
#nav ul ul li {
	background: #a93b43;
	margin-right: -8px;
	position: relative;
}
#nav ul ul li:first-child a {
	border-top: none;
}
#nav ul ul li.last {
	border-bottom: none;
}
#nav ul ul li a {
	display: inline-block;
	line-height: 20px;
	font-size: 16px;
	padding: 6px 15px;
	min-width: 100px;
	border-style: solid;
	border-width: 1px 0;
	border-color: #89181e transparent #b95560;
}
#nav ul ul li a:hover {
	background: #92292f;
}
/*---end Nav---*/

/*---Main---*/
#main {
	background: url(images/bg-main.png) repeat-y 0 0;
	width: 944px;
	position: relative;
	z-index: 40;
}
#content {
	float: left;
	width: 665px;
	padding-left: 19px;
}
#content h1 {
	background: url(images/bg-h1_1.png) no-repeat 0 0;
	width: 349px;
	height: 46px;
	margin: 0 auto;
	padding-left: 5px;
	font: normal 22px/50px 'AtlasRegular';
	color: #9dc1f2;
	text-align: center;
	position: relative;
}
#content h1 span {
	text-shadow: -2px 2px 0 #2f4c76;
	display: inline-block;
}
#content h1 + #content-inner {
	margin-top: 0;
}
#content-inner {
	background: url(images/content-top.png) no-repeat 0 0;
	width: 665px;
	padding-top: 21px;
	margin-top: 12px;
}
#content-bot {
	background: url(images/content-bot.png) no-repeat 0 100%;
	width: 665px;
	padding-bottom: 35px;
	margin-bottom: 12px;
}
#content-area {
	background: url(images/content-bg.png) repeat-y 0 0;
	padding: 18px 44px 0 54px;
	min-height: 590px;
}
.b-article {
	background: url(images/dots.gif) repeat-x 0 0;
	padding: 30px 0 24px;
}
#content-area .b-article:first-child {
	background: none;
	padding-top: 0;
}
.b-article .b-img {
	float: left;
	width: 151px;
	height: 152px;
	border: 8px solid #d7a625;
}
.b-article .b-img img {
	width: 147px;
	height: 148px;
	border: 2px solid #fcea79;
}
.b-article .b-img + .b-info {
	margin-left: 201px;
}
.b-article .b-info h2 {
	font: bold 24px/25px Georgia, serif;
	margin: 5px 0 10px;
	color: #cf6e00;
}
.b-article .b-info h2 a {
	color: #cf6e00;
}
.b-article .b-info h2 a:hover {
	text-decoration: underline;
}
.b-article .b-info p {
	margin: 5px 0;
}
.b-article .b-buts-panel {
	margin-top: 15px;
}
.b-article .b-buts-panel a {
	display: inline-block;
	height: 44px;
	margin-right: 8px;
}
.b-article .b-buts-panel a.enroll {
	background: url(images/but-enroll.png) no-repeat 0 0;
	width: 131px;
}
.b-article .b-buts-panel a.info {
	background: url(images/but-info.png) no-repeat 0 0;
	width: 103px;
}
.b-article .b-buts-panel a.tickets {
	background: url(images/but-tickets.png) no-repeat 0 0;
	width: 131px;
}
/*---end Main---*/

/*---Sidbar---*/
#sidebar {
	float: right;
	width: 242px;
}
.b-subscribe {
	background: url(images/bg-subscribe.png) no-repeat 0 0;
	width: 214px;
	height: 84px;
	margin-top: 15px;
	padding-left: 16px;
	position: relative;
}
.b-subscribe-text {
	font: normal 13px 'HumanaSansITC';
	color: #fcea79;
	padding: 15px 0 8px;
}
.b-subscribe-field .text {
	background: #fbffb2;
	border: solid #c47500;
	border-width: 2px 2px 0 0;
	height: 17px;
	width: 158px;
	padding: 3px;
	font-size: 14px;
	vertical-align: middle;
	margin-right: 6px;
	letter-spacing: -1px;
}
.b-subscribe-field .submit {
	background: url(images/but-submit.png) no-repeat 0 0;
	width: 30px;
	height: 30px;
	border: none;
	vertical-align: middle;
	cursor: pointer;
}
.b-sponsors {
	background: url(images/sponsors.png) no-repeat 0 0;
	width: 213px;
	padding-left: 7px;
	height: 146px;
	margin: 12px 0 10px 5px;
	text-align: center;
}
.b-sponsors .name {
	font: normal 14px 'AtlasRegular';
	text-align: center;
	color: #2c3c67;
	padding-top: 25px;
}
.b-sponsors a {
	font: bold 19px Georgia, serif;
	color: #e5e8ff;
	display: inline-block;
	padding-top: 11px;
}
.b-sponsors a:hover {
	text-decoration: underline;
}
.b-sidebar-links {
	background: url(images/bg-sidebar-list.png) no-repeat 0 0;
	width: 229px;
	height: 328px;
	padding-top: 1px;
}
.b-sidebar-links ul {
	list-style: none;
	margin: 18px 0 0 8px;
	width: 204px;
	padding: 0;
}
.b-sidebar-links ul li {
	padding-left: 48px;
	font: normal 13px/16px 'HumanaSansITC';
	color: #fcea79;
	margin-bottom: 10px;
}
.b-sidebar-links ul li a {
	font-weight: bold;
	font-size: 17px;
	color: #a93b43;
}
.b-sidebar-links ul li a:hover {
	text-decoration: underline;
}
.b-sidebar-links ul li.facebook {
	background: url(images/icon-facebook.png) no-repeat 0 0;
	line-height: 28px;
}
.b-sidebar-links ul li.calendar {
	background: url(images/icon-calendar.png) no-repeat 0 0;
	line-height: 28px;
	padding-bottom: 8px;
}
.b-sidebar-links ul li.member {
	background: url(images/icon-chair.png) no-repeat 0 8px;
	padding-top: 10px;
}
.b-sidebar-links ul li.donation {
	background: url(images/icon-prize.png) no-repeat 0 5px;
	padding-top: 12px;
}
/*---end Sidbar---*/

/*---Footer---*/
#footer {
	background: url(images/bg-footer.png) no-repeat 0 0;
	width: 944px;
	height: 126px;
	position: relative;
	padding-bottom: 33px;
}
.footer-left {
	background: url(images/pigs.png) no-repeat 0 0;
	float: left;
	margin-left: 66px;
	padding-left: 187px;
	padding-top: 22px;
	height: 108px;
	font: normal 20px/27px 'AtlasRegular';
	color: #7ea9e5;
	text-align: center;
	letter-spacing: -1px;
}
.footer-left span {
	text-shadow: -2px 2px 0 #2f4c76;
	display: inline-block;
}
#footer-message {
	font: bold 15px/24px 'HumanaSansITC';
	width: 412px;
	color: #1f2f4d;
	margin-left: 450px;
	text-align: center;
	padding-top: 30px;
}
.footer-nav {
	width: 412px;
	margin-left: 450px;
	text-align: center;
	padding-top: 3px;
}
.footer-nav ul {
	margin: 0;
	list-style: none;
	display: inline;
	font-size: 0;
}
.footer-nav ul li {
	display: inline;
	font-size: 15px;
}
.footer-nav ul a {
	font: bold 15px/18px 'HumanaSansITC';
	color: #b2d1ff;
}
.footer-nav ul a:hover {
	text-decoration: underline;
}
.footer-nav ul li span {
	color: #1f2f4c;
	font-weight: bold;
	padding: 0 10px;
}
/*---end Footer---*/

/*---Redesign2---*/
.camps #content-area {
	padding: 0 21px 0 32px;
}
.b-story {
	background: #82d6cd;
	width: 612px;
	min-height: 600px;
}
.b-story-inner {
	padding: 5px 33px 15px;
	font-size: 11px;
	color: #222d4c;
}
.f12px {
	font-size: 12px !important;
}
.accordion {
	margin: 20px 0;
}
.accordion h4 {
	background: #5cbbc1 url(images/arrow-right.gif) no-repeat 9px 50%;
	padding-left: 25px;
	height: 23px;
	font-weight: normal;
	margin: 0 0 1px 0;
}
.accordion h4.ui-state-active {
	background-image: url(images/arrow-down.gif);
}
.accordion h4 a {
	color: #222d4d;
	font-size: 11px;
	line-height: 23px;
}
.accordion div {
	margin: 0;
	padding: 1px 5px 1px 14px;
	font-size: 10px;
}
/*---end Redesign2---*/

/*---Productions---*/
.fridayfun #content h1,
.touring #content h1,
.productions #content h1 {
	background-image: url(images/bg-h1_2.png);
	width: 390px;
}
.touring #content-area,
.productions #content-area {
	padding-bottom: 20px;
}
.b-purchase-ticket {
	background: url(images/bg-purchase-ticket.png) no-repeat 0 0;
	width: 550px;
	height: 160px;
	padding: 10px 25px 0 20px;
	margin: -10px 0 0 -14px;
}
.b-register-classes h2,
.b-purchase-ticket h2 {
	text-align: center;
	font: normal 22px 'AtlasRegular';
	color: #d7a625;
	margin: 0 0 10px;
}
.b-type-register .b-col > div,
.b-type-purchase > div {
	display: inline-block;
	font: normal 17px 'HumanaSansITC';
	color: #3b5f94;
}
.b-type-register .b-col > div b,
.b-type-purchase > div b {
	font-size: 21px;
}
.b-type-purchase .b-online {
	width: 302px;
	text-indent: 25px;
}
.b-type-purchase .b-phone {
	width: 225px;
	margin-left: 18px;
}
.b-time-purchase {
	text-align: center;
	font: normal 14px 'HumanaSansITC';
	color: #3b5f94;
	padding-top: 18px;
}
h2.season {
	background: url(images/bg-season.png) no-repeat 0 0;
	width: 324px;
	height: 59px;
	margin: 10px auto;
	text-align: center;
}
h2.comingup span,
h2.season span {
	font: normal 22px/55px 'AtlasRegular';
	text-shadow: -2px 2px 0 #a65e0c;
	display: inline-block;
	color: #fbffb2;
}
h2.comingup + .b-article,
h2.season + .b-article {
	background-position: 0 100%;
	padding: 10px 0;
}
.productions .b-article p {
	margin: 5px 0 20px;
	color: #3b5f94;
}
.touring .b-article p {
	color: #3b5f94;
}
.productions .b-article .b-buts-panel {
	text-align: center;
	padding-top: 10px;
}
.productions .b-article .b-buts-panel a {
	margin-right: 0;
}
.touring .b-article .b-info h2,
.productions .b-article .b-info h2 {
	margin-top: 0;
}
.touring #content-area > h2 {
	font: normal 22px 'AtlasRegular';
	color: #fbffb2;
	text-align: center;
	margin: 0 0 5px;
}
.touring h2 + .b-article {
	background: none;
	padding-top: 15px;
}
.b-info ul {
	padding-left: 17px;
	color: #3b5f94;
	margin: 0;
}
.b-info ul li {
	padding: 2px 0;
}
/*---end Productions---*/

/*---About Us---*/
.classes #content h1,
.about #content h1 {
	background-image: url(images/bg-h1_3.png);
	width: 217px;
}


.membership #content h1 {
	background-image: url(images/bg-h1_5.png);
	width: 233px;
	padding-left: 2px;
}

.b-about {
	padding: 0 8px 25px;
}
.b-about p {
	margin: 5px 0 22px;
}
.classes p a,
.b-article p a,
.b-about p a {
	text-decoration: none;
	color: #b2366a;
	border-bottom: 1px dashed #b2366a;
	white-space: nowrap;
}
.classes p a:hover,
.b-article p a:hover,
.b-about p a:hover {
	border-bottom: none;
}
.b-about .ocu-title {
	display: block;
	margin: 22px auto;
}
.b-about .map {
	display: block;
	margin: 0 auto;
	border: 8px solid #d7a625;
}
.about .b-article {
	background-position: 0 100%;
	padding: 24px 0 30px;
}
.about #content-area .b-article:first-child {
	background: url(images/dots.gif) repeat-x 0 100%;
}
.staff #content h1 {
	background-image: url(images/bg-h1_4.png);
	width: 157px;
}
.board #content h1 {
	background-image: url(images/bg-h1_1.png);
	width: 349px;
	margin-top: 10px;
}
.b-about dl {
	background: url(images/dots.gif) repeat-x 0 100%;
	margin: 0;
	padding: 10px 0 35px 20px;
}
.b-about dl.last {
	background: none;
}
.b-about dl + dl {
	padding-top: 30px;
}
.b-about dl dt {
	clear: both;
	float: left;
	font: normal 19px/120% Georgia;
	color: #515894;
	width: 190px;
	padding: 4px 0;
}
.b-about dl dd {
	float: left;
	font: normal 12px/27px Verdana;
	color: #a93b43;
	text-transform: uppercase;
	padding: 4px 0;
	margin-right: -10px;
	word-wrap: break-word;
}
.b-about h2 {
	font: bold italic 16px/120% Georgia;
	color: #cf760f;
	margin: 0;
	padding: 25px 0 0 20px;
}
/*---end About Us---*/

/*---Classes---*/
.classes #content h1 {
	margin-top: 10px;
}
.b-register-classes {
	background: url(images/bg-register-classes.png) no-repeat 0 0;
	width: 543px;
	height: 397px;
	margin: -10px 0 15px -14px;
	padding: 10px 20px 0 32px;
	text-align: center;
}
.b-register-classes p {
	font: normal 14px 'HumanaSansITC';
	color: #3b5f94;
}
.b-register-classes h2 {
	margin-bottom: 5px;
}
.b-register-classes h2 + p {
	margin-top: 0;
}
.b-type-register .b-col {
	width: 305px;
	float: left;
}
.b-type-register .b-col:first-child {
	width: 220px;
	padding-right: 18px;
}
.b-type-register .b-col > div {
	display: block;
	padding-bottom: 20px;
}
.b-type-register .b-mail {
	margin-top: -5px;
}
.b-type-register .b-mail:first-line {
	line-height: 210%;
}
.b-register-links {
	background: url(images/dots.gif) repeat-x 0 0;
	margin: 5px 0 0;
}
.b-register-links ul {
	float: left;
	list-style: none;
	text-align: left;
	padding: 0;
	margin-right: 10px;
}
.b-register-links ul.last {
	margin-right: 0;
}
.b-register-links ul li {
	background: url(images/classes-arrow.png) no-repeat 0 1px;
	padding-left: 20px;
	margin: 12px 0;
}
.b-register-links ul li a {
	color: #b2366a;
	font: normal 17px/18px 'HumanaSansITC';
}
.b-register-links ul li a:hover {
	text-decoration: underline;
}
.b-register-links ul.last li {
	margin-top: 30px;
}
.classes h2.season {
	background-image: url(images/bg-season2.png);
	width: 351px;
}
.classes h2.season + p {
	margin-top: 15px;
}
.classes h2.season + p + .b-article {
	padding-top: 15px;
}
.classes .b-article {
	background: none;
	padding-top: 10px;
	padding-bottom: 20px;
}
.classes p {
	color: #3b5f94;
}
.classes .b-article p {
	margin-bottom: 15px;
}
.classes .b-buts-panel {
	float: right;
	margin: 0 0 10px 10px;
}
/*---end Classes---*/

/*---Fairy Tale---*/
.b-fairy-tale {
	background: url(images/bg-fairy-tale.jpg) no-repeat 0 0;
	width: 613px;
	height: 706px;
	margin: -20px 0 0 -22px;
}
.b-fairy-tale-inner {
	padding: 210px 64px 0;
	text-align: center;
}
.b-fairy-tale-inner h2 {
	font: normal 21px/25px RoskellRegular;
	color: #6f7d3c;
}
.b-fairy-tale-inner ul li,
.b-fairy-tale-inner p {
	font: normal 12px/15px ATAdminister, "Times New Roman", serif;
	color: #633413;
}
.b-fairy-tale-inner p {
	margin: 15px 0;
}
.b-fairy-tale-inner ul li b,
.b-fairy-tale-inner p b {
	font-size: 13px;
	color: #ab5b00;
}
.b-fairy-tale-inner ul {
	list-style: none;
	padding: 0;
	margin: 0;
}
.b-fairy-tale-inner ul li {
	display: inline-block;
	padding: 0 20px;
}
.b-fairy-tale-inner .committee:first-line {
	line-height: 200%;
}
/*---end Fairy Tale---*/


/*---Friday Fun Night---*/
.b-description {
	background: url(images/bg-description.png) no-repeat 0 0;
	width: 550px;
	height: 143px;
	padding: 1px 25px 0 20px;
	margin: -10px 0 0 -14px;
}
.b-description p {
	font: normal 19px/24px 'HumanaSansITC';
	color: #3b5f94;
	text-align: center;
}
.b-description p:first-letter {
	font: normal 20px/24px 'AtlasRegular';
	color: #608fcc;
	letter-spacing: -2px;
}
h2.comingup {
	background: url(images/bg-comingup.png) no-repeat 0 0;
	width: 234px;
	height: 59px;
	margin: 10px auto;
	text-align: center;
}
.fridayfun .b-article .b-info {
	padding-bottom: 15px;
}
.fridayfun .b-article .b-info p {
	color: #3b5f94;
	margin: 20px 0;
}
.fridayfun .b-article .b-info h2 + p {
	margin-top: 5px;
}
